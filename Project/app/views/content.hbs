<div class="back-div">
    {{#each content}}
    <div class="product-item">
        <img class="mini-pic"
            src="https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/2020-audi-s8-rear-motion-1561988432.jpg?crop=0.655xw:0.599xh;0.345xw,0.347xh&resize=480:*"
            alt="Card image cap">
        <a id="link-item" href="/content/{{this.id}}">
            <label class="brand_label">{{'car_model.car_brand.brand_name'}}</label>
            <label class="model_label"> {{'car_model.model_name'}}</label>
            <label class="cylinders_label">
                {{'car_cylinder.cylinders_marking'}}{{'car_cylinder.cylinders_count'}}</label>
            <label class="volume_label"> - {{'engine_volume.volume'}}</label>
            <label class="body_label">({{'car_body.body_name'}},</label>
            <label class="fuel_label">{{'car_fuel.fuel_name'}},</label>
            <label class="drive_label"> {{'car_drive.drive_name'}} привод)</label>
        </a>
    </div>
    {{/each}}
</div>
<div class="down-div">
    
</div>